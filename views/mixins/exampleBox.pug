mixin exampleBox(component, entry, template)
	li.Component.drag.draggable-item(id= component.id, data-component="exampleBox" data-order= component.componentOrder)
		.sub-sortable-container
			h2 Example <span style="font-size:15px">&#9432;</span>
			.exampleBoxBlock
				.exampleBoxTitle 
				.exampleBoxContent
				.editorSortable
					.simpleEditor
						.editorTools.unreset(id= "toolbar_" +component.id)
						if (component.componentContent)
							.editorText.unreset(id= "editor_" +component.id) !{component.componentContent}
						else
							.editorText.unreset(id= "editor_" +component.id)
								p Write something here...