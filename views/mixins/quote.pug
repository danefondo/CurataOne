mixin quote(component, entry, template)
	li.Component.drag.draggable-item(id= component.id)
		.sub-sortable-container
			h2 Quote
			.quoteBlock
				.quoteContent
				.editorSortable
					.simpleEditor
						.editorTools.unreset(id= "toolbar_" +component.id)
						.editorText.unreset(id= "editor_" +component.id)
							p This is sample text of editor.